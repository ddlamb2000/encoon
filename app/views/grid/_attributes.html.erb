<article id="grid-<%= filters_uuid(@grid, @filters) %>">
  <%= render :partial => 'list', :locals => {:dialog => true} %>
</article>
<script>
  $("#dialog-list").dialog({
    resizable: false,
    closeOnEscape: true,
    hide: "puff",
    width: 650,
    title : "<%= @grid %>",
    buttons: {
      "<%= t 'actions.cancel' %>": function() {$("#dialog-list").dialog("close");}
    }
  });
</script>
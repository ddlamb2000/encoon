$(document).ready(function(){
  $("#search-form")
    .live("ajax:beforeSend", function(evt, xhr, settings){
      $('#search-list').empty();
    })
    .live("ajax:success", function(evt, data, status, xhr){
      $('#search-list').empty();
      $('#search-list').append('<%= escape_javascript(render :partial => "search_list") %>');
    });
});
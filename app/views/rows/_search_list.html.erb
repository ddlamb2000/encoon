<%= render :partial => "list",            :locals => {               :embedded => false,              :grid => @grid,               :search => params[:search],               :filters => params[:filters],               :table_row_count => @grid.row_count(params[:filters]),              :table_rows => @grid.row_all(params[:filters], params[:search], params[:page], true),               :table_columns => @grid.filtered_columns } %>